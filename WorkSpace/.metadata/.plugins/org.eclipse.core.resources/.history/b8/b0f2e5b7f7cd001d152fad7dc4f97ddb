package pack04_char;

public class Ex01_char {
	public static void main(String[] args) {
		//문자 데이터 아비 : char(2byte)
		//하나의 글자만 저장: 유니코드
		//변수에 값을 할당(대입)할 때는 작은 따옴표를 사용한다.
		//작은 따옴표 == 싱글 코테이션 =='
		// 소문자 a를 담고
		//문자 데이터타입 char를 이용하여 초기화해보기.
		char c1 = 'a';
		int iNum = 10;
		char cha2 = 'B';
		char cha3 = '★';
		char cha4 = '가';
		//char charError = ''; 아무것도 없는것은 x
		System.out.println(iNum);
		System.out.println(c1);
		System.out.println(cha2);
		System.out.println(cha3);
	}
}
