package pack08_casting;

public class Ex_Casting {
	public static void main(String[] args) {
		int iNum = 10;
		double dNum = iNum;
		System.out.println(dNum);
		
		double dNum1 = 30.5;
		int iNum2 = (int) dNum1;
		
		System.out.println(iNum2);
		
		double dNum2 = 302.5;
		float fNum = (float) dNum2;
		System.out.println(fNum);
		
		int iNum3 = 100;
		double dNum3 = iNum3;
		System.out.println(dNum3);
		String str1 = "가나다라";
		char chr= str1 
				
	}
}	

