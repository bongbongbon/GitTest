
public class Test06 {
	public static void main(String[] args) {
		int korScore , mathScore , engScore , sciScore , phiScore ; 
		korScore = 80 ;
		mathScore = 88 ; 
		engScore = 90 ;
		sciScore = 95 ;
		phiScore = 100 ;
		System.out.println(korScore);
		
		// 형변환(Casting) : 서로 다른 타입의 변수를 변환시켜 사용함.
		// ex) int <-> double , String <-> int
		// int < double 
		int iValue = 10;
		double dValue = iValue + 0.5;
		System.out.println("iValue 값 : " + iValue); // 아무것도 한것없음 ==> 자동 형변환
		System.out.println("dValue 값 : " + dValue); // 아무것도 한것없음 ==> 자동 형변환
		
	}
}
